            },
            {
              "title": "Enter Customer Paycode",
              "body": "Input the customer's Paycode into the appropriately labelled field"
            },
            {
              "title": "Enter Initiator's Phone Number",
              "body": "Enter the phone number of the person who generated the Paycode"
            },
            {
              "title": "Enter Customer's Details",
              "body": "Input the details of the customer"
            },
            {
              "title": "Enter Amount",
              "body": "Enter the amount to be withdrawn. The amount must match the amount set when the paycode was generated"
            },
            {
              "title": "Review Transaction Details",
              "body": "Review and confirm the transaction details including the cash out amount"
            },
            {
              "title": "Enter Customer's PIN",
              "body": "Input the customer's PIN into the appropriately labelled field"
            },
            {
              "title": "Dispense Cash",
              "body": "Once the transaction is successful, dispense cash to the customer"
            }
          ]
        }
      }
